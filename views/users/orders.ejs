<%- include('../partials/user-header')%>
      </header>
      <div class="bg-light py-3">
        <div class="container">
          <div class="row">
            <div class="col-md-12 mb-0">
              <a href="/">Home</a> <span class="mx-2 mb-0">/</span>
              <strong class="text-black">Contact</strong>
            </div>
          </div>
        </div>
      </div>

      <div class="site-section">
        <div class="container">
          <div class="row">
            <div class="col-lg-12">
              <div class="table-responsive">
                <table class="table">
                  <thead>
                    <tr>
                      <th width="30%">Product</th>
                      <th width="20%">Unit Price</th>
                      <th width="20%">Quantity</th>
                      <th width="10%">Total</th>
                      <th width="20%">Status</th>
                      <th width="30%" class="pl-5">Action</th>
                    </tr>
                  </thead>
                  <tbody>
                    <%for(let i=0; i < products.length; i++){%>
                    <tr>
                      <td>
                        <a class="itemside" href="#">
                          <div class="left">
                            <img
                              src="<%=products[i].product.image[0]%>"
                              width="40"
                              height="40"
                              class="img-xs"
                              alt="Item"
                            />
                          </div>
                          <div class="info"><%=products[i].product.title%></div>
                        </a>
                      </td>
                      <td>$<%=products[i].product.price%></td>
                      <td><%=products[i].quantity%></td>
                      <td>
                        $<%=products[i].quantity * products[i].product.price%>
                      </td>
                      <td class="text-end">
                        <%if(products[i].status==="Delivered" ){%>
                          <p class="text-success">Delivered</p>

                        <%}else if(products[i].status==="Returned" ||
                        products[i].status==="cancelled" ){%>
                        <a                          
                          style="
                            color:red;
                            border: none;
                            width: 100px;
                          "
                        >
                        <%=products[i].status%>
                        </a>
                        <%}else{%>
                        <button
                          class="ml-1"
                          style="border: none; background-color: white;"
                          id="order"
                          onclick="cancelOrder('<%=products[i]._id%>')"
                        >
                          <p class="text-success"><%=products[i].status%></p>
                        </button>
                        <%}%>
                      </td>
                      <td class="text-end">
                        <%if(products[i].status==="Delivered" ){%>
                        <button
                          class="ml-1"
                          style="border: none; background-color: white;"

                          id="order"
                          onclick="returnOrder('<%=products[i]._id%>')"
                        >
                          <p class="text-warning">Return</p>
                        </button>

                        <button
                          class="ml-1"
                          style="border: none; background-color: white;"
                          id="order"
                          onclick="downloadReport('<%=order[i]._id%>')"
                        >
                          <p class="text-info">Invoice</p>
                        </button>
                        <%}else if(products[i].status==="Returned" ||
                        products[i].status==="cancelled" ){%>
                        <a
                        class="ml-3"                          
                          style="
                            background-color:white;
                            border: none;
                            width: 100px;
                          "
                          href="/shop"
                        >
                          Shop
                        </a>
                        <%}else{%>
                        <button
                          class="ml-1"
                          style="border: none; background-color: white;"
                          id="order"
                          onclick="cancelOrder('<%=products[i]._id%>')"
                        >
                          <p class="text-danger">Cancel</p>
                        </button>
                        <%}%>
                      </td>
                    </tr>
                    <%}%>
                  </tbody>
                </table>
              </div>
              <!-- table-responsive// -->
            </div>
          </div>
        </div>
      </div>

      <footer class="site-footer border-top">
        <div class="container">
          <div class="row">
            <div class="col-lg-6 mb-5 mb-lg-0">
              <div class="row">
                <div class="col-md-12">
                  <h3 class="footer-heading mb-4">Navigations</h3>
